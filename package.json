{
  "name": "notional",
  "version": "0.0.1",
  "description": "An unofficial Notion client",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js"
  },
  "author": "TimRobinson1",
  "license": "ISC",
  "jest": {
    "rootDir": "src",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/(__tests__/)?.*(\\.|/)(test|spec))\\.(j|t)s?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
